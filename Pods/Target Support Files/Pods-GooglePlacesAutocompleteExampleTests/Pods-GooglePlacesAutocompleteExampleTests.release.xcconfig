FRAMEWORK_SEARCH_PATHS = $(inherited)  "$(SDKROOT)/Developer/Library/Frameworks" "$(PLATFORM_DIR)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 FB_REFERENCE_IMAGE_DIR="\"$(SOURCE_ROOT)/../FBSnapshotTest/ReferenceImages\""
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/FBSnapshotTestCase" "${PODS_ROOT}/Headers/Public/OHHTTPStubs"
