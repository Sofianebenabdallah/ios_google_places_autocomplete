OTHER_LDFLAGS = $(inherited) -ObjC -l"FBSnapshotTestCase" -l"OHHTTPStubs" -framework "CFNetwork" -framework "Foundation" -framework "XCTest"
PODS_ROOT = ${SRCROOT}/../Pods